{"ast":null,"code":"import Tabulator from \"tabulator-tables\"; // Import Tabulator\nimport \"tabulator-tables/dist/css/tabulator.min.css\"; // Import Tabulator CSS\n\nexport default {\n  name: \"ShippingCompanyTable\",\n  data() {\n    return {\n      companies: [{\n        code: 0,\n        shortName: \"User\",\n        fullName: \"One\"\n      }, {\n        code: 1,\n        shortName: \"User\",\n        fullName: \"Two\"\n      }, {\n        code: 10,\n        shortName: \"User\",\n        fullName: \"Three\"\n      }, {\n        code: 11,\n        shortName: \"User\",\n        fullName: \"Four\"\n      }, {\n        code: 12,\n        shortName: \"User\",\n        fullName: \"Five\"\n      }]\n    };\n  },\n  mounted() {\n    this.initTabulator(); // Initialize Tabulator on mount\n  },\n  methods: {\n    initTabulator() {\n      // Create Tabulator instance\n      new Tabulator(\"#edi-table\", {\n        data: this.companies,\n        // Set table data\n        layout: \"fitColumns\",\n        // Fit columns to container width\n        columns: [{\n          title: \"Shipping Company Code\",\n          field: \"code\",\n          headerSort: false\n        }, {\n          title: \"Transport Company Abbreviation\",\n          field: \"shortName\",\n          headerSort: false\n        }, {\n          title: \"Shipping Company Name\",\n          field: \"fullName\",\n          headerSort: false\n        }],\n        height: \"300px\" // Optional: set table height\n      });\n    }\n  }\n};","map":{"version":3,"names":["Tabulator","name","data","companies","code","shortName","fullName","mounted","initTabulator","methods","layout","columns","title","field","headerSort","height"],"sources":["D:\\VS_Code\\EMTR\\jgs-assist\\src\\components\\ShippingCompanyTable.vue"],"sourcesContent":["<template>\r\n  <div class=\"shipping-table-container\">\r\n    <fieldset class=\"shipping-fieldset\">\r\n      <h2>EDI Conversion Table</h2>\r\n      <!-- Tabulator container -->\r\n  